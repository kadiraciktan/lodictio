{
  "name": "lodictio",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "build:api": "nx build api && docker build -t lodictio-api -f ./apps/api/Dockerfile . && docker compose up --detach",
    "test": "nx test",
    "typeorm": "typeorm-ts-node-commonjs -d ./libs/main-database/data-source.ts",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./libs/main-database/src/migrations/$npm_config_name",
    "migration:generate": "nx build main-database && npm run typeorm -- migration:generate ./libs/main-database/src/migrations/$npm_config_name",
    "migration:run": "nx build main-database && npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "gql": "npx graphql-codegen --config apps/api/codegen.yml"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/base": "^5.0.0-alpha.100",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.8",
    "@mui/styled-engine-sc": "^5.10.6",
    "@nestjs/apollo": "^10.0.19",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.0.21",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@nrwl/nest": "^14.5.4",
    "@nrwl/next": "14.5.4",
    "apollo-server-core": "^3.10.1",
    "apollo-server-express": "^3.10.1",
    "autoprefixer": "^10.4.8",
    "axios": "^1.0.0",
    "core-js": "^3.6.5",
    "graphql": "^16.6.0",
    "graphql-tools": "^8.3.3",
    "next": "^12.3.1",
    "pg": "^8.8.0",
    "postcss": "^8.4.16",
    "postgres": "^3.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.7",
    "rxjs": "^7.0.0",
